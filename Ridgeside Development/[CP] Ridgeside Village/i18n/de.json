//Spring
"Acorn.Dialogue.spring_1": "Eichel mag Frühling!$h#$b#Eichel mag Bäume!$h",
"Acorn.Dialogue.spring_12": "Eichel mag Hühner!$h#$b#Eichel LIEBT Eier!$h",
"Acorn.Dialogue.spring_23": "Eichel mag keine Blumen...$a",

//Summer
"Acorn.Dialogue.summer_1": "Eichel denkt, es ist jetzt heiß!$s",
"Acorn.Dialogue.summer_10": "Eichel will Suppe!$h#$b#Eichel LIEBT Suppe!!$h",
"Acorn.Dialogue.summer_27": "Eichel wird Quallen sehen!$h",

//Fall
"Acorn.Dialogue.fall_1": "*keuch* Hübsche Blätter!$h",
"Acorn.Dialogue.fall_15": "Eichel will Spiele spielen!$h",
"Acorn.Dialogue.fall_26": "Eichel wird sich erschrecken!$s",

//Winter
"Acorn.Dialogue.winter_1": "Brrrrr...$s",
"Acorn.Dialogue.winter_7": "Eichel wird zum kalten Fest gehen!$s",
"Acorn.Dialogue.winter_24": "GESCHENKE!!!$h",

//Randomized
"Acorn.Dialogue.random.1" : "Eichel wird Pipo helfen!$h",
"Acorn.Dialogue.random.2" : "Eichel mag schreiben!$h",
"Acorn.Dialogue.random.3" : "Eichel, Chip Chip!$h",
"Acorn.Dialogue.random.4" : "$d joja#Dummes Joja!$a|Eichel liebt Wildtiere!$h",
"Acorn.Dialogue.random.5" : "Eichel hat @ ein Geschenk besorgt!$h [388]",
"Acorn.Dialogue.random.6" : "Eichel und Pipo machen heute ein Rennen!$h",
"Acorn.Dialogue.random.7" : "Augenklappe ist cool!$h#$b#Eichel ist cool!$4",
"Acorn.Dialogue.random.8" : "Pipo ist Eichhorns bester Freund!$h",
"Acorn.Dialogue.random.9" : "Was$u",
"Acorn.Dialogue.random.10" : " Pipo duscht heute?$s",
"Acorn.Dialogue.random.11" : "@!$h",
"Acorn.Dialogue.random.12" : "@, DU BIST SO COOL!$4!",
"Acorn.Dialogue.random.13" : "Chip Chip!$h",
"Acorn.Dialogue.random.14" : "Heute wachte Acorn auf, als ein Eichhörnchen an seinem Kopf kaute!$s#$b#ACORN SCHMECKT NICHT GUT!$a",
"Acorn.Dialogue.random.15" : "Du!$u#$b#Bringst du Stock für Eichel!?$h",
"Acorn.Dialogue.random.16" : "Manchmal wird Acorn von Eichhörnchen gejagt.$s#$b#Dann jagt Pipo Eichhörnchen!$h ",
"Acorn.Dialogue.random.17" : "Pipo gab Acorn Hut, chip chip!$h#$b#Jetzt ist Acorn sehr cool!!$4",
